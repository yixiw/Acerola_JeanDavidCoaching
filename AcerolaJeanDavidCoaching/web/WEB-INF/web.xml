<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletCreerExercice</servlet-name>
        <servlet-class>servlet.exercice.ServletCreerExercice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gestionExercice</servlet-name>
        <jsp-file>/jsp/gestionExercice.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>servlet.exercice.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAfficherObjectifExe</servlet-name>
        <servlet-class>servlet.exercice.ServletAfficherObjectifExe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAjouterObjectifExe</servlet-name>
        <servlet-class>servlet.exercice.ServletAjouterObjectifExe</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletCreerExercice</servlet-name>
        <url-pattern>/ServletCreerExercice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gestionExercice</servlet-name>
        <url-pattern>/gestionExercice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAfficherObjectifExe</servlet-name>
        <url-pattern>/ServletAfficherObjectifExe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAjouterObjectifExe</servlet-name>
        <url-pattern>/ServletAjouterObjectifExe</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
